@model ProfileViewModel

@{
    ViewData["Title"] = "Profile";
}

<div class="user-details container">
<h1>Account Details</h1>
<table class="table table-striped">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        
            <tr>
                <td>@Model.FirstName</td>
                <td>@Model.LastName</td>
                <td>@Model.Email</td>
            </tr>  
    </tbody>
</table>
</div>




